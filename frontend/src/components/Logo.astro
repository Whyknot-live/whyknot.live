---
export interface Props {
    width?: number;
    height?: number;
    className?: string;
    title?: string;
}

const {
    width = 42,
    height = 42,
    className = "",
    title = "WhyKnot Y Knot Logo",
} = Astro.props;
---

<img
    src="/images/logo-y-knot-transparent.png"
    alt={title}
    width={width}
    height={height}
    class={className}
/>
